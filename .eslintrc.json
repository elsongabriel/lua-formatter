{
  "extends": ["unobtrusive", "unobtrusive/import"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "@elsongabriel/prettier": "warn"
  },
  "overrides": [
    {
      "files": "tests/**/*.js",
      "globals": {
        "run_spec": true
      }
    }
  ]
}
