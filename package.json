{
  "name": "@elsongabriel/lua-formatter",
  "version": "1.0.2",
  "description": "Prettier Lua Formatter",
  "main": "src",
  "scripts": {
    "test": "cross-env AST_COMPARE=1 jest",
    "test:watch": "cross-env AST_COMPARE=1 jest --watchAll --updateSnapshot",
    "lint:fix": "npx eslint --fix",
    "prettier": "prettier --plugin=. --parser=lua",
    "prettier:test": "prettier --plugin=. --parser=lua --write \"lua-tests/**/*.lua\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elsongabriel/lua-formatter.git"
  },
  "keywords": [
    "prettier",
    "lua",
    "formatter",
    "otservbr",
    "opentibiabr",
    "otserv",
    "elson"
  ],
  "author": "Elson Costa",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "luaparse": "0.2.1"
  },
  "devDependencies": {
    "@elsongabriel/prettier": "^1.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-unobtrusive": "^1.2.5",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.10.0"
  },
  "peerDependencies": {
    "prettier": ">=2.0.0"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest.*.config.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/elsongabriel/lua-formatter/issues"
  },
  "homepage": "https://github.com/elsongabriel/lua-formatter#readme",
  "directories": {
    "test": "tests"
  }
}
